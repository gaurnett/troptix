      "dest": "./api/events.js"
    },
    {
      "src": "/api/events/:getEventsType/:id",
      "dest": "./api/events.js"
    },
    {
      "src": "/api/orders",
      "dest": "./api/orders.js"
    },
    {
      "src": "/api/orders/:getOrdersType/:id",
      "dest": "./api/orders.js"
    },
    {
      "src": "/api/users",
      "dest": "./api/users.js"
    },
    {
      "src": "/api/users/:getUsersType/:id",
      "dest": "./api/users.js"
    },
    {
      "src": "/api/ticketTypes",
      "dest": "./api/ticketTypes.js"
    },
    {
      "src": "/api/tickets",
      "dest": "./api/tickets.js"
    },
    {
      "src": "/api/tickets/:userId",
      "dest": "./api/tickets.js"
