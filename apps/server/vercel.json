{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "./api/test/handler.js"
    },
    {
      "source": "/api/events",
      "destination": "./api/events.js"
    },
    {
      "source": "/api/events/:getEventsType/:id",
      "destination": "./api/events.js"
    },
    {
      "source": "/api/orders",
      "destination": "./api/orders.js"
    },
    {
      "source": "/api/orders/:getOrdersType/:id",
      "destination": "./api/orders.js"
    },
    {
      "source": "/api/users",
      "destination": "./api/users.js"
    },
    {
      "source": "/api/users/:getUsersType/:id",
      "destination": "./api/users.js"
    },
    {
      "source": "/api/ticketTypes",
      "destination": "./api/ticketTypes.js"
    },
    {
      "source": "/api/tickets",
      "destination": "./api/tickets.js"
    },
    {
      "source": "/api/tickets/:userId",
      "destination": "./api/tickets.js"
    },
    {
      "source": "/api/delegatedUsers",
      "destination": "./api/delegatedUsers.js"
    },
    {
      "source": "/api/delegatedUsers/:eventId",
      "destination": "./api/delegatedUsers.js"
    },
    {
      "source": "/api/promotions",
      "destination": "./api/promotions.js"
    },
    {
      "source": "/api/promotions/:getPromotionsType/:eventId/:code",
      "destination": "./api/promotions.js"
    },
    {
      "source": "/api/promotions/:eventId",
      "destination": "./api/promotions.js"
    },
    {
      "source": "/api/contact",
      "destination": "./api/contact.js"
    }
  ]
}